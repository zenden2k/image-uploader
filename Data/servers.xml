	<!--
		Since 1.3.x release server declaration for "urlshortening" server node must be "Server2",
	-->
	<Server2 Name="u.to" Authorize="0" Type="urlshortening">
		<Actions>
			<Action
				Type="post"
				Url="https://u.to"
				PostParams="url=$(_ORIGINALURL);a=add"
				RegExp="\('#shurlout'\)\.val\('(.+)'\)\.show"
				AssignVars="surl:0"
			/>
		</Actions>
		<Result DirectUrlTemplate="$(surl)" />
	</Server2>
	<Server2 Name="tinyurl.com" Authorize="0" Type="urlshortening" >
		<Actions>
			<Action
				Type="get"
				Url="http://tinyurl.com/api-create.php?url=$(_ORIGINALURL|urlencode)"
				RegExp='tinyurl\.com\/(\w+)'
				AssignVars="surl:0"
			/>
		</Actions>
		<Result DirectUrlTemplate="https://tinyurl.com/$(surl)" />
	</Server2>
