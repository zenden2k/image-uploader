			</Action>
			<Action Type="get" Url="$(ResultsPage)" RegExp="\[IMG\](.*?)\[([\s\S]*?)\[URL=(.*?)\]\[IMG\](.*?)\[" AssignVars="Image:0;View:2;Thumb:3;"/>
