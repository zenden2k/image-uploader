<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Servers>
	<Server Name="6url.ru" Authorize="0" NeedPassword="0" Type="urlshortening" >
		<Actions>
			<Action 
				Type="get"
				Url="http://6url.ru/?api=$(_ORIGINALURL|urlencode)"
				RegExp='(.+)'
				AssignVars="surl:0"
			/>
		</Actions>
		<Result DirectUrlTemplate="$(surl)" />
	</Server>
</Servers>