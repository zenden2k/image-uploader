<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Servers>
	<Server Name="link.ac" Authorize="0" Type="urlshortening" >
		<Actions>
			<Action 
				Type="post"
				Url="link.ac"
				PostParams="qmode=cutlink;needurl=undefined;url=$(_ORIGINALURL)"
				RegExp='clipmain1\.setText\("(.+)"\)'
				AssignVars="surl:0"
			/>
		</Actions>
		<Result DirectUrlTemplate="$(surl)" />
	</Server>
</Servers>